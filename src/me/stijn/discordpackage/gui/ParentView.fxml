<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="580.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.stijn.discordpackage.controllers.ParentView">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="loadView" mnemonicParsing="false" onAction="#onLoadFileViewButton" text="Load file" />
            <Button fx:id="messageStats" mnemonicParsing="false" onAction="#onMessageStatsButton" text="Message stats" />
            <Button fx:id="activityStats" mnemonicParsing="false" onAction="#onActivityStatsButton" text="Activity stats" />
            <Button fx:id="actions" mnemonicParsing="false" onAction="#onActionsChartButton" text="Actions chart" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
